// Enum with the severity levels for alerts and callouts
export enum CalloutSeverityLevel {
  HELPFUL = 'HELPFUL',
  IMPORTANT = 'IMPORTANT',
  CRITICAL = 'CRITICAL',
}

export enum AlertSeverityLevel {
  Note = 'Note',
  Tip = 'Tip',
  TODO = 'TODO',
  QUESTION = 'QUESTION',
  Summary = 'Summary',
  TLDR = 'TLDR',
  CRITICAL = 'CRITICAL',
  IMPORTANT = 'IMPORTANT',
  HELPFUL = 'HELPFUL',
}
